pipeline{
  agent any
  environment{
          PATH="/opt/maven/bin:$PATH"
     }
      stages{
        stage("git checkout"){
           steps{
             git 'https://github.com/venkat040/myweb.git'
               }
           }
             stage("build"){
           steps{
             sh 'mvn clean package'
               }
           }
       }
   }
